  print_endline "}"

let print_store_equal () =
  Printf.printf "let store_equal (store1 : store) (store2 : store) : bool =\n";
  List.map
    (fun (_l, r, equal, _diff, _add_diff, _sub_diff) ->
       Printf.sprintf "(%s store1.%s store2.%s)" equal r r)
    store_fields
  |> String.concat " && " |> print_string

