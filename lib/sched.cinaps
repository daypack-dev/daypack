  print_endline "}"

let print_sub_diff_store () =
  Printf.printf
    "let sub_diff_store (diff : store_diff) (store : store) : store =\n";
  print_endline "{";
  List.iter
    (fun (_l, r, _equal, _diff, _add_diff, sub_diff) ->
       Printf.printf "%s = %s diff.%s_diff store.%s;\n" r sub_diff r r)
