type int64 <ocaml from="Int64"> = abstract

type ('task_seg_related_data, 'time) fixed = {
  task_seg_related_data : 'task_seg_related_data;
  start : 'time;
}

type ('task_seg_related_data, 'time_seg) shift = {
  task_seg_related_data_list : 'task_seg_related_data list;
  time_segs : 'time_seg list;
  incre : int64;
}

type split_count = [
  | Max_split of int64
  | Exact_split of int64
]

type ('task_seg_related_data, 'time_seg) split_and_shift = {
  task_seg_related_data : 'task_seg_related_data;
  time_segs : 'time_seg list;
  incre : int64;
  split_count : split_count;
  min_seg_size : int64;
  max_seg_size : int64 option;
}

type ('task_seg_related_data, 'time_seg) split_even = {
  task_seg_related_data : 'task_seg_related_data;
  time_segs : 'time_seg list;
  buckets : 'time_seg list;
  incre : int64;
}

type ('task_seg_related_data, 'time_seg) time_share = {
  task_seg_related_data_list : 'task_seg_related_data list;
  time_segs : 'time_seg list;
  interval_size : int64;
}

type ('task_seg_related_data, 'time, 'time_seg) push_toward = {
  task_seg_related_data : 'task_seg_related_data;
  target : 'time;
  time_segs : 'time_seg list;
  incre : int64;
}

type ('task_seg_related_data, 'time, 'time_seg) sched_req_data_unit_skeleton = [
  | Fixed of ('task_seg_related_data, 'time) fixed
  | Shift of ('task_seg_related_data, 'time_seg) shift
  | Split_and_shift of ('task_seg_related_data, 'time_seg) split_and_shift
  | Split_even of ('task_seg_related_data, 'time_seg) split_even
  | Time_share of ('task_seg_related_data, 'time_seg) time_share
  | Push_toward of ('task_seg_related_data, 'time, 'time_seg) push_toward
]
